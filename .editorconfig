root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
max_line_length = 100
trim_trailing_whitespace = true

[*.java]
indent_size = 4

[*.proto]
indent_size = 2

[*.sh]
indent_size = 2

[{*.yml, *.yaml}]
indent_size = 2

[*.md]
indent_size = 2
